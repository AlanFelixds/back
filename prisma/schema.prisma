generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Servidor {
  id       String @id @default(uuid())
  email    String
  password String

  @@map("servidor")
}

model Events {
  id           Int      @id @default(autoincrement())
  name         String
  date         DateTime
  teacher      String
  teacherEmail String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  // participants Participants @relation(fields: [participantsID], references: [id])
  // participantsID Int
}

model Participants {
  id    Int    @id @default(autoincrement())
  name  String
  email String
  // events Events[]
}

